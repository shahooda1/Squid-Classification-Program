
class domain:
    
    def __init__(self, domainName):
        
        self.domainName = domainName
        
    def domainInfo(self):
        
        print("Domain Information:")
        print("Name:", self.domainName)


class kingdom(domain):
    
    def __init__(self, domainName, kingdomName):
        
        super().__init__(domainName)
        self.kingdomName = kingdomName
    

        

colossalSquid = kingdom("Eukarya", "Animalia")

colossalSquid.domainInfo()
        




    
